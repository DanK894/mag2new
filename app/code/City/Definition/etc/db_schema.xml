<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
<table name="city_table" resource="default" engine="innodb" comment="Declarative Schema Definition">
    <column xsi:type="smallint" name="city_id" padding="6" unsigned="false" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="varchar" name="city_ref" nullable="false" length="255" comment="CITY REF"/>
    <column xsi:type="varchar" name="city_name_ua" nullable="false" length="255" comment="CITY NAME UA"/>
    <column xsi:type="varchar" name="city_name_ru" nullable="false" length="255" comment="CITY NAME RU"/>
    <column xsi:type="varchar" name="city_area" nullable="false" length="255" comment="CITY AREA"/>
    <column xsi:type="smallint" name="city_id_np" padding="6" unsigned="false" nullable="false" comment="ID NP"/>
    <column xsi:type="varchar" name="city_area_description_ua" nullable="false" length="255" comment="CITY AREA NAME UA"/>
    <column xsi:type="varchar" name="city_area_description_ru" nullable="false" length="255" comment="CITY AREA NAME RU"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
        <column name="city_id"/>
    </constraint>
</table>
</schema>
